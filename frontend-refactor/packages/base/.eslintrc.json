{
    "parser": "@typescript-eslint/parser",
    "extends": ["next/core-web-vitals", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "max-len": [
            "error",
            {
                "code": 120,
                "tabWidth": 4
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "tabWidth": 4
            }
        ]
    }
}
