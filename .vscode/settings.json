{
  "files.exclude": {
    "frontend/": true,
    "vulkan/": true,
    "vulkan-server/": true,
    "vulkan-dagster/": true,
    "upload-svc/": true,
  },
  "python.defaultInterpreterPath": ".venv/bin/python",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.requireConfig": true
}
