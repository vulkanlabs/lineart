{
    "files.exclude": {
        "vulkan/": true,
        "vulkan-server/": true,
        "vulkan-dagster/": true,
        "vulkan-public/": true,
    },
    "python.defaultInterpreterPath": "vulkan/.venv/bin/python",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "prettier.requireConfig": true
}